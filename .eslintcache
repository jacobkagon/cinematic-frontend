      )\n  }, [addReview]);\n\n  const filterReviews = (event) => {\n    const newReviews = movieReviews.filter((review) => (\n      review.id !== event\n    ))\n    if(event !== undefined) {\n    handleMovieReviews([newReviews])\n    // handleMovieReviews(newReviews)\n    }\n  }\n\n  const deleteReview = (event) => {\n   fetch(`http://localhost:3000/api/v1/reviews/${event}`, {\n   method: 'DELETE',\n   headers: {Authorization: `Bearer ${token}`}\n})\nhandleAddReview(true)\n\n\n  }\n\n  return (\n    <div className={classes.root}>\n      <GridList className={classes.gridList} cols={2.5}>\n        {movieReviews \n          ? movieReviews.map((review) => (\n              <GridListTile >\n                <p>{review.body}</p>\n{ review.user.id == localStorage.getItem('user_id') ?\n                <Button color=\"secondary\" value={review.id} onClick={(event) => deleteReview(event.currentTarget.value)}>\n                  x\n                </Button> : null\n\n        }\n               <Link to= {'/'+review.user.username+'/'+review.user.id}>\n                <GridListTileBar\n               \n                  title={<Avatar className={classes.orange}>{review.user.username[0]}</Avatar>}\n                  classes={{\n                    root: classes.titleBar,\n                    title: classes.title,\n                  }}\n                  \n                  actionIcon={\n                    <IconButton aria-label={`star ${review}`}>\n                      <StarBorderIcon className={classes.title} />\n                    </IconButton>\n                  }\n                />\n               </Link>\n              </GridListTile>\n            ))\n          : null}\n      </GridList>\n    </div>\n  );\n}\n","/Users/jacobkagon/Flatiron/code/cinematic/cinematic-frontend/src/components/AddWatchlist.jsx",["182","183","184","185","186"],"/Users/jacobkagon/Flatiron/code/cinematic/cinematic-frontend/src/recoil/login.js",["187","188"],"import {atom} from 'recoil'\nimport {sele